
public class InsertionSort {
    public void insertionSort(int[] dizi){
        for (int i = 1; i < dizi.length; i++) {
            int temp = dizi[i];
            int j = i - 1;
            
            while(j > -1 && dizi[j] > temp){
                dizi[j+1] = dizi[j];
                j--;
            }
            dizi[j+1] = temp;
        }
    }
}
